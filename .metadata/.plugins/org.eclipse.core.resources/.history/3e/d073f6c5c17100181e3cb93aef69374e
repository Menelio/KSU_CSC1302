package mod3;

public class Staff extends Employee {
	//instance variables
	String title;
	
	//constructor
	public Staff(String name, String email, double salary, String title) {
		super(name, email, salary);
		this.title = title;
	}
	//Getters and setters
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	
	
}
