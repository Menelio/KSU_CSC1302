package mod2;

public class test {

	public static void main(String[] args) {
		Time time = new Time();
		System.out.println(time.getHour());
		System.out.println(time.getMinute());
		System.out.println(time.getSeconds());
		
		time.setTime(1200000000);
		System.out.println();
		
		System.out.println(time.getHour());
		System.out.println(time.getMinute());
		System.out.println(time.getSeconds());
		
	}

}
