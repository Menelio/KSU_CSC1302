package mod3;

import java.sql.Date;

public class Employee extends Person {
	//instance variables
	double salary;
	Date dateHired;
	
	//constructor
	public Employee(String name, String email, double salary) {
		super(name, email);
		this.salary = salary;
	}


	//Getters and setters
	public double getSalary() {
		return salary;
	}	
	public void setSalary(double salary) {
		this.salary = salary;
	}
	
	
}
