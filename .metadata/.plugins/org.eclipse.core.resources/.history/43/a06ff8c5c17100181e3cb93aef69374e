package mod3;

public class Student extends Person {
	//instance variables
	String classStatus;
	
	//constructor
	public Student(String name, String email, String classStatus) {
		super(name, email);
		this.classStatus = classStatus;
	}
	//Getters and setters
	public String getClassStatus() {
		return classStatus;
	}
	public void setClassStatus(String classStatus) {
		this.classStatus = classStatus;
	}
	
	
}
