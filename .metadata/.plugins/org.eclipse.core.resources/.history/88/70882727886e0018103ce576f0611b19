package mod2;

import java.sql.Date;

public class test {

	public static void main(String[] args) {
		Time time = new Time();
		System.out.println(time.getHour());
		System.out.println(time.getMinute());
		System.out.println(time.getSeconds());
		
		time.setTime(1200000000);
		System.out.println();
		
		System.out.println(time.getHour());
		System.out.println(time.getMinute());
		System.out.println(time.getSeconds());
		System.out.println("\n In Milly "+System.currentTimeMillis());
		java.sql.Time time2 = new java.sql.Time(System.currentTimeMillis());
		
		System.out.println("\n"+time2.getHours());
		
				 
		 
	}

}
